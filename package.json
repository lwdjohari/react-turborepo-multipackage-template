{
  "name": "root",
  "private": true,
  "workspaces": [
    "web",
    "@sharedscope/*"
  ],
  "repository": "git@github.com:lwdjohari/react-turborepo-multipackage-template.git",
  "author": {
    "name": "Linggawasistha Djohari",
    "email": "linggawasistha.djohari@outlook.com"
  },
  "license":"MIT",
  "version": "0.2.3",
  "scripts": {
    "build-lib": "turbo run build   --filter @sharedscope/core --filter @sharedscope/api --filter @sharedscope/ui",
    "clean-lib": "turbo run clean  --filter @sharedscope/core --filter @sharedscope/api --filter @sharedscope/ui",
    "build": "turbo run build --filter web",
    "clean": "turbo run clean --filter web",
    "reset": "turbo run reset --filter web",
    "build-all": "turbo run build  --filter @sharedscope/core --filter @sharedscope/api --filter @sharedscope/ui   --filter web",
    "clean-all": "turbo run clean  --filter @sharedscope/core --filter @sharedscope/api --filter @sharedscope/ui   --filter web",
    "start": "turbo run dev --filter web",
    "lint": "turbo run lint --filter web",
    "preview": "turbo run preview --filter web",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "reset-ws": "turbo run reset && rm -rf ./node_modules ./package-lock.json ./yarn.lock",
    "proto-gen": "turbo run proto-gen --filter @sharedscope/api",
    "test-lib": "turbo run test --concurrency 1 --filter @sharedscope/core --filter @sharedscope/api "
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "devDependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/joy": "^5.0.0-beta.27",
    "@mui/material": "^5.15.10",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.14",
    "@storybook/react": "^7.6.14",
    "@storybook/react-vite": "^7.6.14",
    "@storybook/test": "^7.6.14",
    "@storybook/theming": "^7.6.14",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.10.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^7.6.14",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.1.2",
    "turbo": "latest",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-lib-inject-css": "^1.3.0"
  },
  "packageManager": "yarn@4.1.0"
}
